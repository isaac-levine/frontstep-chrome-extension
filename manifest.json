{
  "manifest_version": 3,
  "name": "Frontstep",
  "version": "0.0.1",
  "description": "A browser extension for Frontstep",
  "permissions": [
    "identity",
    "storage",
    "cookies",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": ["https://*.clerk.com/*", "https://mail.google.com/*"],
  "action": {
    "default_popup": "popup.html"
  }
}
